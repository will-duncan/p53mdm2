    RowBox[{"LPT", "*", "LMcT"}], " ", "<", " ", "thetaPMn"}], " ", "&&", " ", 
   RowBox[{
    RowBox[{"UPT", "*", "LMcT"}], " ", "<", " ", "thetaPMn"}], " ", "&&", " ", 
   RowBox[{"thetaPMn", " ", "<", " ", 
    RowBox[{"LPT", "*", "UMcT"}]}], " ", "&&", " ", 
   RowBox[{"thetaPMn", " ", "<", " ", 
    RowBox[{"UPT", "*", "UMcT"}]}], " ", "&&", " ", 
   RowBox[{"0", " ", "<", " ", "LPT"}], " ", "&&", " ", 
   RowBox[{"LPT", " ", "<", " ", "UPT"}], " ", "&&", " ", 
   RowBox[{"0", " ", "<", " ", "LMcT"}], " ", "&&", " ", 
   RowBox[{"LMcT", " ", "<", " ", "UMcT"}], " ", 
   RowBox[{"(*", 
    RowBox[{"Mn", " ", "orderings"}], "*)"}], "&&", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"LMcP", " ", "-", " ", 
     RowBox[{"UPT", "*", "UMcT"}]}], " ", "<", " ", "thetaMnMc"}], " ", "&&", 
   "    ", 
   RowBox[{
    RowBox[{"UMcP", " ", "-", " ", 
     RowBox[{"UPT", "*", "UMcT"}]}], " ", "<", " ", "thetaMnMc"}], " ", "&&", 
   "  ", 
   RowBox[{
    RowBox[{"LMcP", " ", "-", " ", 
     RowBox[{"UPT", "*", "LMcT"}]}], " ", "<", " ", "thetaMnMc"}], " ", "&&", 
   " ", 
   RowBox[{
    RowBox[{"UMcP", " ", "-", " ", 
     RowBox[{"LPT", "*", "UMcT"}]}], " ", "<", " ", "thetaMnMc"}], " ", "&&", 
   "\[IndentingNewLine]", 
   RowBox[{"thetaMnMc", " ", "<", " ", "thetaMcMc"}], " ", 
   RowBox[{"(*", 
    RowBox[{"back", " ", "unfolding"}], "*)"}], "&&", " ", 
   "\[IndentingNewLine]", " ", 
   RowBox[{"thetaMcMc", " ", "<", " ", 
    RowBox[{"UMcP", " ", "-", " ", 
     RowBox[{"UPT", "*", "LMcT"}]}]}], " ", "&&", "  ", 
   RowBox[{"thetaMnMc", " ", "<", " ", 
    RowBox[{"UMcP", " ", "-", " ", 
     RowBox[{"LPT", "*", "LMcT"}]}]}], " ", 
   RowBox[{"(*", 
    RowBox[{"Mc", " ", "orderings"}], "*)"}], "&&", "\[IndentingNewLine]", 
   RowBox[{"LMcP", " ", "<", " ", "UMcP"}], " ", "&&", " ", 
   "\[IndentingNewLine]", 
   RowBox[{"0", " ", "<", " ", "LMcP"}], "  ", "&&", " ", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"LMcP", " ", "-", " ", 
     RowBox[{"UPT", "*", "UMcT"}]}], " ", ">", " ", "0"}], "  ", "&&", 
   "\[IndentingNewLine]", 
   RowBox[{"LPMn", " ", "<", " ", "1"}], " ", "&&", " ", 
   RowBox[{"UPMn", " ", ">", " ", "1", " ", 
    RowBox[{"(*", 
