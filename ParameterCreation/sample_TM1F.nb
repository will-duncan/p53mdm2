Cell[BoxData[
 RowBox[{"cond", ":=", 
  RowBox[{
   RowBox[{"0", " ", "<", " ", "LPMn"}], " ", "&&", " ", 
   RowBox[{"LPMn", " ", "<", " ", "thetaMcP"}], " ", "&&", " ", 
