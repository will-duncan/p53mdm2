   RowBox[{"thetaMcP", " ", "<", " ", "UPMn"}], " ", "&&", " ", 
   RowBox[{"thetaPT", " ", "<", " ", "UPMn"}], " ", "&&", " ", 
   RowBox[{"LPMn", " ", "<", " ", "UPMn"}], " ", "&&", " ", 
   RowBox[{"thetaMcP", " ", "<", " ", "thetaPT"}], " ", 
   RowBox[{"(*", 
    RowBox[{"P", " ", "orderings"}], "*)"}], "&&", "\[IndentingNewLine]", 
